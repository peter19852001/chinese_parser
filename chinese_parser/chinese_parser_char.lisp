;; also convert the list into hash-table for faster checking, and make it symmetric
(defparameter *cn-zh-char-pairs*
  (let ((pairs (make-hash-table :test 'eql)))
    (dolist (p *cn-zh-char-pairs-list* pairs)
      (let ((cs (coerce (car p) 'character))
            (cts (coerce (cdr p) 'list)))
        (dolist (ct cts)
          (pushnew ct (gethash cs pairs nil) :test #'eql)
          ;; also index use the traditional character as key
          (pushnew cs (gethash ct pairs nil) :test #'eql))))))
;;
(defun cn-char-equal (ca cb)
  ;; return nil if not matched
  ;; return score if matched (-1 for now, a tiny penalty)
  (cond ((char-equal ca cb) 0)
        ((member cb (gethash ca *cn-zh-char-pairs* nil)
                 :test #'eql)
         -1)
        (t nil)))

