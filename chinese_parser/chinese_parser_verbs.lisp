(def-word-class verb-n (verbs)
  ;; to distinguish between verb-x with a noun and a single verb
  ;; should we also record the noun?
  ((verb ;; to hold the tag of the verb
    :initarg :verb
    :initform nil
    :reader verb-n-verb)
   (obj ;; to hold the tag of the object
    :initarg :obj
    :initform nil
    :reader verb-n-obj)
   ))

(def-word-class verb0 (verbs)
  ;; a verb that has not taken an object yet, sort of leave a slot there
  ((verb ;; to hold the tag of the verb
    :initarg :verb
    :initform nil
    :reader verb0-verb)
   ))

(def-word-class verb-bian4-adj (verbs)
  ;; 變 + adj, e.g. 變好, 變壞, 變高, 變婑
  ;; could even be '變 得 很好' as a result of verb + supp rules.
  ;; note that this normally does not take a noun.
  ;; and when used in subj-verb, would require the adj to agree on the subject
  ((adj ;; to hold the adj suffix
    :initarg :adj
    :initform nil
    :reader verb-bian4-adj-adj)))
