(def-tagged-words *verb-close-supp-list* *verb-close-supp-hash*
  ((verb-close-supp verbs) ;; close supplements of verbs
   "完" "掉" ;; similar in meaning, 'used up', or 'done something'

   "住" ;; similar to 着, could mean 'on going', or physically stopping

   (verb-close-supp-cheng2
    "成" ;; to mark the change, or transformation
    )

   (verb-close-supp-mark-place
    ;; usually mark a place
    (verb-close-supp-bian4
     ;; 'tried all' whatever, could be places even if the original
     ;; verb does not take a place, e.g. 吃遍 中國
     "遍")
    (verb-close-supp-from-to
     ;; usually mark direction or places, either from or to
     ;; could allow place or (usually) animate
     (verb-close-supp-xiang4 "向")
     (verb-close-supp-yu1 "於")
     "自")
    )

   (verb-close-supp-dong3
    ;; to mark understanding, which is a verb in itself, seems to be
    ;; used with only a few verbs
    "懂")
   (verb-close-supp-zuo4
    ;; similar to 成, also mark change or transformation, but much
    ;; less common, so give a larger penalty. seems better be one
    ;; piece with the main verb
    "作")
   (verb-close-supp-dong4
    ;; with movement verbs, to mark movement, but seems better be one
    ;; piece with the main verb
    "動")
   (verb-close-supp-yi3
    ;; seems better be one piece with main verb
    "以")
   (verb-close-supp-zhe) ;; this is in the grammar, because the tval is AS
   )
  )

;; end verb-close-supp-list
