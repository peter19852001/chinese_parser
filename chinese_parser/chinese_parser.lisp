(defmacro check-score (f t1 t2)
  `(,f (make-instance ',t1) (make-instance ',t2)))

(defun check-pair (f w1 w2 s1 s2)
  (let ((t1 (tags-of-parse w1 s1))
        (t2 (tags-of-parse w2 s2)))
    (format t "~A: ~S~%~A: ~S~%"
            w1 t1 w2 t2)
    (dolist (ta t1)
      (dolist (tb t2)
        (format t "~A (~A , ~A): ~A~%"
                f (type-of ta) (type-of tb) (funcall f ta tb))))
    (terpri)))
