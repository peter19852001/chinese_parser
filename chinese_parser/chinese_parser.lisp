;; next construct a simple naive bayes classifier to classify the NR and NN
;; P(N|W) = P(N)P(W|N)/P(W)
;; P(W|N) = \prod_i P(w_i|N) assuming conditional independence
;; P(N=nr|W)/P(N=nn|W) = {P(N=nr)P(W|N=nr)}/{P(N=nn)P(W|N=nn)} = {P(N=nr)/P(N=nn)} \prod_i {P(w_i|N=nr)/P(w_i|N=nn)}
;; P(N=nr)/P(N=nn) = {#nr/#total}/{#nn/#total} = {#nr}/{#nn}
;; so calculate the log (base e) ratio of P(c|N=nr)/P(c|N=nn) for each character c,
;; together with log ratio of P(N=nr)/P(N=nn), we can calculate log {P(N=nr|W)/P(N=nn|W)}
(defun cal-name-char-log-ratio (counts-lst n-NR n-NN)
  (let ((ratios (make-hash-table))
        (pseudo-count 0.5))
    (dolist (x counts-lst) ;; (c NR-count . NN-count)
      (setf (gethash (char (car x) 0) ratios)
            (log (/ (/ (+ (cadr x) pseudo-count) n-NR)
                    (/ (+ (cddr x) pseudo-count) n-NN)))))
    ratios))

(defparameter *name-char-log-ratio*
  (cal-name-char-log-ratio *name-char-counts-lst*
                           *n-NR-words*
                           *n-NN-words*))


(defun predict-NR (str)
  ;; TODO: turn this into one that identifies all potential names
  ;; return true or nil
  (let ((log-odd (log (coerce (/ *n-NR-words* *n-NN-words*)
                              'double-float))))
    (dolist (c (coerce str 'list))
      (incf log-odd (gethash c *name-char-log-ratio* 0)))
    ;; predict name if probability > 0.5, which has log odds of 0
    (values (> log-odd 0) log-odd)))

(defparameter *name-forbidden-chars*
  ;; note that '.' and '*' are allowed, as it may occur as part of the name of a foreigner
  ;; '-', '/',':' are also allowed.
  (coerce "“”…,\"'`「」~。$%！？、;(){}<>[]《》■" 'list))
(defparameter *name-A-Z-chars*
  (coerce "abcdefghijklmnopqrstuvwxyz" 'list))
(let ((ret-tag (make-instance 'name)))
  (defun match-human-name (words)
    ;; assume what follows is a name, for Chinese names, we limit
    ;; ourselves to 1 or 2 characters, because it is the majority.
    ;; return list of (score match-len . tag)
    (let ((ret nil)
          (log-odd (log (coerce (/ *n-NR-words* *n-NN-words*)
                                'double-float))))
      (when words
        (incf log-odd (gethash (car words) *name-char-log-ratio* 0))
        (push `(,(if (> log-odd 0)
                     *c-human-name*
                     ;; *c-likely-name* has higher penalty than *c-human-name*
                     *c-likely-name*)
                 1 . ,ret-tag) ret)
        (when (cdr words)
          (incf log-odd (gethash (cadr words) *name-char-log-ratio* 0))
          (push `(,(* 2
                      (if (> log-odd 0)
                          *c-human-name*
                          ;; *c-likely-name* has higher penalty than *c-human-name*
                          *c-likely-name*))
                   2 . ,ret-tag) ret)))
      ret))
  (defun match-name (words)
    ;; look till the end of the list words, which is a list of characters.
    ;; return list of (score match-len . tag) for each possible lengths.
    ;; forbidden most punctuation characters as part of a name.
    ;; If not forbidden, returns the tag as name.
    ;; If forbidden, returns the tag as nil.
    (let ((ret nil)
          (allowed? t)
          ;; An English word only ends at either space, or just before
          ;; non-A-Z character, and has at least two character.
          (a-z? t)
          (Eng-name-len nil)
          (n-Eng-words 0))
      (flet ((may-add-acc-Eng-word ()
               ;; end previous English word?
               (when (and Eng-name-len (> Eng-name-len 1))
                 (incf n-Eng-words)
                 (push `(,(* n-Eng-words *c-Eng-word*)
                          ,Eng-name-len . ,ret-tag)
                       ret)))
             (may-add-cur-Eng-word (cur-len)
               (when (and a-z? (> cur-len 1)
                          ;; previouse character not an end point.
                          ;; either no previous point, or not at previous character
                          (not (eql Eng-name-len cur-len)))
                 (incf n-Eng-words)
                 (push `(,(* n-Eng-words *c-Eng-word*)
                          ,cur-len . ,ret-tag)
                       ret)))
             )
        (do ((log-odd (log (coerce (/ *n-NR-words* *n-NN-words*)
                                   'double-float)))
             ;;
             (ws words (cdr ws))
             (len 1 (1+ len)))
            ((null ws)
             ;; last check of English word
             (may-add-acc-Eng-word)
             ;; current word
             ;; NOTE: here len is one past the length, adjust it
             (may-add-cur-Eng-word (- len 1))
             ;;
             ret)
          ;;
          (when allowed?
            (cond ((member (car ws) *name-forbidden-chars* :test #'cn-char-equal)
                   (may-add-acc-Eng-word)
                   (may-add-cur-Eng-word (- len 1))
                   ;;
                   (setf allowed? nil
                         a-z? nil
                         Eng-name-len nil))
                  ((not a-z?) nil)
                  ;; still a-z and space only.
                  ;; The convention is to attach space to the previous word.
                  ((member (car ws) *name-A-Z-chars* :test #'cn-char-equal)
                   (may-add-acc-Eng-word)
                   )
                  ((cn-char-equal (car ws) #\ )
                   ;; record a possible point for end of English word
                   (setf Eng-name-len len))
                  ;; not a-z? anymore
                  (t (may-add-acc-Eng-word)
                     (may-add-cur-Eng-word (- len 1))
                     (setf a-z? nil
                           Eng-name-len nil))
                  ))
          ;;
          (incf log-odd (gethash (car ws) *name-char-log-ratio* 0))
          ;; TODO: the thresholds need to be tuned
          ;; TODO: consider having more categories
          ;; TODO: use more sophisticated model, which also considers the position of some characters
          (cond (a-z?
                 ;; only predict using these scores if it is not A-Z only
                 nil)
                ((and allowed?
                      (> log-odd 0)
                      ;; some characters (such as '.') should not appear at the end of a name, but rather at the middle
                      (not (cn-char-equal (car ws) #\. )))
                 (push `(,(* len *c-likely-name*) ,len . ,ret-tag) ret))
                (allowed? (push `(,(* len *c-unlikely-name*) ,len . ,ret-tag) ret))
                (t (push `(,(* len *c-unlikely-name*) ,len . nil) ret)))))))
  )
