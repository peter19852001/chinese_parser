;;; The main parsing function
(defun test-parse (str &optional (verbose t) (start-symbol 's) get-partial-parses)
  ;; use the sample grammar
  (let ((*penalty-func* #'test-penalty-func)
        (*score-lower-threshold* *c-total-limit*)
        (best-parses nil)
        (best-score nil)
        (partial-parses-vec nil))
    (clear-up-ind)
    (clear-more-trait-with-hash)
    (when verbose (format t "Input: ~A~%" str))
    (multiple-value-bind (parses score partial-parses)
        (earley-parser *chinese-g* start-symbol (coerce str 'list))
      (setf best-parses parses
            best-score score)
      (when get-partial-parses
        (setf partial-parses-vec partial-parses)))
    ;; now the parse returns only one compact parse which may contain
    ;; alternatives
    (when verbose (format t "Parse: ~A~%~%" best-parses))
    (values best-parses best-score partial-parses-vec)))

(defun join-splice (tree)
  ;; (X1 ... Xn (:splice Y1 ... Ym) Z1 ... Zk) => (X1 ... Xn Y1 ... Ym Z1 ... Zk)
  (if (atom tree)
      tree
      ;; (consp tree) ;; top level
      (let ((acc nil))
        (dolist (x tree (nreverse acc))
          (cond ((atom x) (push x acc))
                ((eq (car x) :splice)
                 (labels ((walk (ls)
                            (dolist (z ls)
                              (cond ((atom z) (push z acc))
                                    ((eq (car z) :splice)
                                     (walk (cdr z)))
                                    (t (push (join-splice z) acc))))))
                   (walk (cdr x))))
                (t (push (join-splice x) acc)))))))

;;; The parsing function to get CTB output
(defun ctb-parse (str &optional (verbose t) (start-symbol 's))
  (let ((*CTB-output* t))
    (join-splice (test-parse str verbose start-symbol))))

(defun test-parses (strs)
  ;; use the sample grammar
  (let ((*penalty-func* #'test-penalty-func)
	(*score-lower-threshold* *c-total-limit*))
    (do ((ss strs (cdr ss))
	 (i 0 (1+ i)))
	((null ss))
      (let ((unknowns nil)
	    (str (car ss)))
	(format t "Input ~A: ~A~%" i str)
	(let ((ps (earley-parser *chinese-g* 's (coerce str 'list))))
	  (dolist (p ps)
	    (dolist (x p)
	      (when (and (listp x) (eq (car x) 'unknown))
		(pushnew x unknowns :test #'equal))))
	  (terpri)
	  (format t "Unknowns: ~A~%" unknowns)
	  (terpri))))))
