;;;
(defun match-prefix (ws words)
  ;; both ws and words are list of characters, check whether ws is a prefix of words
  ;; return nil if not matched, the matching length and score if matched
  (do ((score 0)
       (xs ws (cdr xs))
       (L 0 (1+ L))
       (ys words (cdr ys)))
      ((null xs) (values L score))
    (if ys
        (let ((c-score (cn-char-equal (car xs) (car ys))))
          (if c-score
              (incf score c-score)
              (return nil)))
        (return nil))))

(defun match-rep-aa (ws words)
  ;; return nil if not matched, the matching length and score if matched
  (multiple-value-bind (L1 score1) (match-prefix ws words)
    (and L1
	 (let ((xs (nthcdr L1 words)))
	   (and xs
                (multiple-value-bind (L2 score2) (match-prefix ws xs)
                  (and L2 (values (+ L1 L2) (+ score1 score2)))))))))

(defun match-rep-aabb (ws words)
  ;; for each character in ws, match to two character in words
  ;; return nil if not matched, the matching length and score if matched
  (do ((score 0)
       (xs ws (cdr xs))
       (L 0 (+ L 2))
       (ys words (cddr ys)))
      ((null xs) (values L score))
    (if (and ys (cdr ys))
        (let ((s1 (cn-char-equal (car xs) (car ys)))
              (s2 (cn-char-equal (car xs) (cadr ys))))
          (if (and s1 s2)
              (incf score (+ s1 s2))
              (return nil)))
        (return nil))))

(defun best-char-match (c cs)
  ;; cs is a list of (character . value)
  ;; return nil if c is not matched to any of cs
  ;; return the best matched score and the value otherwise
  (let ((score nil)
        (val nil))
    (dolist (mc cs (values score val))
      (let ((s (cn-char-equal c (car mc))))
        (when (and s
                   (or (null score) (> s score)))
          (setf score s
                val (cdr mc)))))))

(defun match-rep-abcab (ws words cs)
  ;; cs is a list of (character . value)
  ;; either two instances of ws separated by one character in cs
  ;; or the first character of ws, followed by one character in cs, followed by one ws
  ;; A C A, AB C AB, A C AB.
  ;; return nil if not matched, the matching length and score if matched.
  ;; also return the matched pattern: i.e. A-C-A, AB-C-AB, A-C-AB. And return the type of the middle C matched.
  (let ((score 0)
	(w (first ws))
        (matched-c nil))
    (if (null (cdr ws)) ;; A C A
        (and words (aif (cn-char-equal w (first words))
                        (incf score it)
                        nil)
             (cdr words) (multiple-value-bind (c-score c-val)
                             (best-char-match (second words) cs)
                           (if c-score
                               (progn
                                 (setf matched-c c-val)
                                 (incf score c-score))
                               nil))
             (cddr words) (aif (cn-char-equal w (third words))
                               (incf score it)
                               nil)
             (values 3 score 'A-C-A matched-c))
        (multiple-value-bind (L1 s1) (match-prefix ws words)
          (if L1 ;; AB C AB
              (let ((xs (nthcdr L1 words)))
                (incf score s1)
                (and xs (multiple-value-bind (c-score c-val)
                            (best-char-match (first xs) cs)
                          (if c-score
                              (progn
                                (setf matched-c c-val)
                                (incf score c-score))
                              nil))
                     (cdr xs)
                     (multiple-value-bind (L2 s2) (match-prefix ws (cdr xs))
                       (and L2 (values (+ L1 1 L2) (+ score s2)
                                       'AB-C-AB matched-c)))))
              ;; A C AB
              (and words (aif (cn-char-equal w (first words))
                              (incf score it)
                              nil)
                   (cdr words) (multiple-value-bind (c-score c-val)
                                   (best-char-match (second words) cs)
                                 (if c-score
                                     (progn
                                       (setf matched-c c-val)
                                       (incf score c-score))
                                     nil))
                   (cddr words)
                   (multiple-value-bind (L2 s2) (match-prefix ws (cddr words))
                     (and L2 (values (+ 2 L2) (+ score s2)
                                     'A-C-AB matched-c)))))))))

;; tagged-word-list is an assoc-list where the car is the class (an
;; instance) of the tag, and the cdr is the word list as in word-list.
;; tagged-word-hash is as in output of word-list-hash.


(defun match-double-char (words)
  (cond ((and words (cdr words))
         (let* ((w1 (first words))
                (w2 (second words))
                (c-score (cn-char-equal w1 w2)))
           (if c-score
               (list (list c-score 2))
               nil)))
        (t nil)))

(defun match-word-list (word-list words)
  (let ((ret nil))
    (dolist (w word-list ret)
      (multiple-value-bind (L s) (match-prefix w words)
        (when L (push (cons s L) ret))))))

(defun match-tagged-word-list (tagged-word-list words)
  ;; return list of (score match-len . tag)
  (let ((ret nil))
    (dolist (tag-ls tagged-word-list ret)
      (dolist (w (cdr tag-ls))
        (multiple-value-bind (L s) (match-prefix w words)
          (when L (push (cons s (cons L (car tag-ls))) ret)))))))

(defun each-first-char (tagged-word-hash first-char func)
  ;; func is (lambda (word-list) ...)
  ;; use the word hash and the first character to reduce the word lists to search
  (aif (gethash first-char tagged-word-hash nil)
       (funcall func it))
  (dolist (c (gethash first-char *cn-zh-char-pairs* nil))
    (aif (gethash c tagged-word-hash nil)
         (funcall func it))))

(defmacro for-words (words tagged-word-hash word &body body)
  (let ((g-words (gensym))
        (g-word-list (gensym)))
    `(let ((,g-words ,words))
       (when ,g-words
         (each-first-char
          ,tagged-word-hash
          (first ,g-words)
          (lambda (,g-word-list)
            (dolist (,word ,g-word-list)
              ;; word is (word . tag)
              ,@body)))))))

(defun match-tagged-word-hash (tagged-word-hash words)
  ;; return list of (score match-len . tag)
  (let ((ret nil))
    (for-words words tagged-word-hash w
      ;; w is (word . tag)
      (multiple-value-bind (L s) (match-prefix (car w) words)
        (when L (push `(,s ,L . ,(cdr w)) ret))))
    ;;
    ret))

(defun match-aa-word-list (word-list words)
  (let ((ret nil))
    (dolist (w word-list ret)
      (multiple-value-bind (L s) (match-prefix w words)
        (when L (push (cons s L) ret)))
      (multiple-value-bind (L s) (match-rep-aa w words)
        (when L (push (cons s L) ret))))))

(defun match-aa-tagged-word-list (tagged-word-list words)
  (let ((ret nil))
    (dolist (tag-ls tagged-word-list ret)
      (dolist (w (cdr tag-ls))
        (multiple-value-bind (L s) (match-prefix w words)
          (when L (push (cons s (cons L (car tag-ls))) ret)))
        (multiple-value-bind (L s) (match-rep-aa w words)
          (when L (push (cons s (cons L (car tag-ls))) ret)))))))

(defun match-aa-tagged-word-hash (tagged-word-hash words)
  (let ((ret nil))
    (for-words words tagged-word-hash w
      ;; w is (word . tag)
      (multiple-value-bind (L s) (match-prefix (car w) words)
        (when L (push `(,s ,L . ,(cdr w)) ret)))
      (multiple-value-bind (L s) (match-rep-aa (car w) words)
        (when L (push `(,s ,L . ,(cdr w)) ret))))
    ;;
    ret))

(defparameter *word-not*
  (mapcar #'cons
          (coerce "不一了" 'list)
          '(bu4 yi1 le)))
(defun annotate-rep-type (word-len matched-pattern matched-char tag)
  ;; in A-C-A, A-C-AB, AB-C-AB rep patterns, annotate the tag as appropriate
  (case matched-pattern
    ((A-C-A AB-C-AB)
     (more-trait tag
                 'rep-pattern matched-pattern
                 'rep-lens (list word-len 1 word-len)
                 'rep-matched-char matched-char))
    ((A-C-AB)
     (more-trait tag
                 'rep-pattern matched-pattern
                 'rep-lens (list 1 1 word-len)
                 'rep-matched-char matched-char))
    (t tag)))
(defun match-verb-hash (tagged-word-hash words)
  ;; verbs have A A, A 不 A, AB 不 AB, A 不 AB,
  ;; A 一 A, AB 一 AB, A 一 AB
  (let ((ret nil))
    (for-words words tagged-word-hash w
      ;; w is (word . tag)
      (multiple-value-bind (L1 s1) (match-prefix (car w) words)
        (when L1
          (push `(,s1 ,L1 . ,(cdr w)) ret)
          (let ((xs (nthcdr L1 words)))
            (when xs
              (multiple-value-bind (L2 s2) (match-prefix (car w) xs)
                (when L2 (push `(,(+ s1 s2) ,(+ L1 L2) . ,(cdr w))
                               ret)))))))
      (multiple-value-bind (L s mp mc)
          (match-rep-abcab (car w) words *word-not*)
        (when L (push `(,s ,L . ,(annotate-rep-type
                                  (length (car w)) mp mc (cdr w)))
                      ret))))
    ;;
    ret))

(defun match-adj-hash (tagged-word-hash words)
  ;; adjectives have AABB, A 不 A, AB 不 AB, A 不 AB
  (let ((ret nil))
    (for-words words tagged-word-hash w
      ;; w is (word . tag)
      (multiple-value-bind (L s) (match-prefix (car w) words)
        (when L (push `(,s ,L . ,(cdr w)) ret)))
      (multiple-value-bind (L s mp mc)
          (match-rep-abcab (car w) words *word-not*)
        (when L (push `(,s ,L . ,(annotate-rep-type
                                  (length (car w))
                                  mp mc (cdr w)))
                      ret)))
      (multiple-value-bind (L s) (match-rep-aabb (car w) words)
        (when L (push `(,s ,L . ,(cdr w)) ret))))
    ;;
    ret))
