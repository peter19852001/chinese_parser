(defun mark-also-verbs (tagged-word-hash)
  ;; copy the tag and set also-verb-p for words that are also verbs.
  ;; modify tagged-word-hash in place.
  (let ((marked-tags (make-hash-table)))
    (flet ((mark (tag)
             (or (gethash tag marked-tags)
                 (let ((new-tag (copy-instance tag)))
                   (setf (also-verb-p new-tag) t)
                   (setf (gethash tag marked-tags)
                         new-tag)))))
      (for-word-list-hash
       tagged-word-hash
       #'(lambda (p)
           ;; p is (word . class-instance)
           (let* ((w (car p))
                  (L (length w))
                  (vs (match-verb-hash *verbs-hash* w)))
             (when (find L vs :key #'second :test #'eql)
               ;; at least one match with full length
               (setf (cdr p) (mark (cdr p)))))))
       )))

(mark-also-verbs *abstract-hash*)
