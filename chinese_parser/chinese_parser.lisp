(defun indent (n-spaces)
  (dotimes (i n-spaces) (princ " ")))
(defun json-char-escape (c)
  (cond ((eql c #\") "\\\"")
        ((eql c #\\ ) "\\\\")
        (t c)))
(defun non-terminal-node-p (tree)
  (and (consp tree)
       (or (symbolp (car tree))
           (detail-info-p (car tree)))))
(defun print-node-head (h)
  (cond ((symbolp h) (format t "{\"name\":\"~A\"" h))
        ((detail-info-p h)
         (format t "{\"name\":\"~A\", \"score\": ~A"
                 (detail-info-name h)
                 (detail-info-penalty h))
         (let ((tag (detail-info-tag h)))
           (cond ((consp tag) ;; list of tags
                  (format t ", \"tag\": [")
                  (do ((cs tag (cdr cs)))
                      ((null cs))
                    (format t "\"~A\"" (type-of (car cs)))
                    (when (cdr cs) (format t ", ")))
                  (format t "]"))
                 ;; need not display for nil
                 ((not (null tag)) ;; a single tag
                  (format t ", \"tag\": [\"~A\"]"
                          (type-of tag))))))))
(defun print-parse-tree-to-json (tree &optional (n-spaces 0))
  ;; each node is a {"name": name, "children": [child1, child2, ...]}
  ;; leaf has no children, and only name
  ;; with minmal indentation.
  ;; for our purpose, there is little need of escaping, 
  (cond ((null tree) (format t "{}"))
        ((characterp tree)
         (format t "{\"name\":\"~A\"}" (json-char-escape tree)))
        ((numberp tree)
         (format t "{\"name\":\"~A\"}" tree))
        ((and (consp tree) (characterp (car tree))) ;; a word
         (format t "{\"name\":\"~{~A~}\"}" (json-char-escape tree)))
        ((non-terminal-node-p tree)
         ;; non-terminal node
         (print-node-head (car tree))
         (when (cdr tree)
           (format t ",~%")
           (indent n-spaces)
           (format t " \"children\":~%")
           (indent n-spaces)
           (format t " [")
           (do ((cs (cdr tree) (cdr cs)))
               ((null cs))
             (print-parse-tree-to-json (car cs) (+ 2 n-spaces))
             (when (cdr cs)
               (format t ",~%")
               (indent (+ 2 n-spaces))))
           (format t "]"))
         (format t "}"))
        (t (error "Unknown node: ~A" tree))))

(defun class-names-to-json (names)
  (format nil "[~{~S~^, ~}]" names))

(defun parse-tree-to-json (tree)
  (with-output-to-string (*standard-output*)
    (print-parse-tree-to-json tree)))
;;;;
(defun member-alternative (x alts)
  ;; alts is normally (:@ alt1 atl2 ...)
  ;; x could be alternatives or other parse tree node
  ;; returns true if every alternative in x (or x itself) is in one of the alternatives in alts
  ;; Note that the alternatives in x and alts may itself be alternatives, but not necessarily in the same order
  (cond ((and (consp x) (eq (car x) :@))
         ;; x is alternative, go through all of them
         (every #'(lambda (z) (member-alternative z alts))
                (cdr x)))
        ;; base case for x, equals some alternative in alts
        ((and (consp alts) (eq (car alts) :@))
         (some #'(lambda (a) (member-alternative x a))
               (cdr alts)))
        ;; alts has only one alternative
        (t (equal-parse x alts))))
(defun equal-parse (p1 p2)
  ;; consider traditional / simplified Chinese as equivalent
  ;; also for :@ nodes, the order of alternatives are irrelevant
  (or (and (consp p1) (consp p2)
           (if (eq (car p1) :@)
               (and (eq (car p2) :@)
                    ;; naive quadratic comparison
                    (member-alternative p1 p2)
                    (member-alternative p2 p1))
               (and (not (eq (car p2) :@))
                    (equal-parse (car p1) (car p2))
                    (equal-parse (cdr p1) (cdr p2)))))
      (and (characterp p1) (characterp p2)
           (cn-char-equal p1 p2))
      (and (stringp p1) (stringp p2)
           (= (length p1) (length p2))
           (every #'cn-char-equal p1 p2))
      (equalp p1 p2)))
