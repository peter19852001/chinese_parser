;; For debugging purposes
(defun debug-test-parse (str &optional (verbose t))
  ;; use the sample grammar
  (let ((*penalty-func* #'test-penalty-func)
	(*score-lower-threshold* *c-total-limit*))
    (when verbose (format t "Input: ~A~%" str))
    (earley-parses *chinese-g* 's (coerce str 'list))))

(defmacro partial-parses-to (var &body body)
  `(defparameter ,var
     (multiple-value-bind (best-parses best-score partial-parses-vec)
         (progn ,@body)
       (declare (ignore best-parses best-score))
       partial-parses-vec)))

(defun completes-of (partial-parses-vec start at &optional nt-name (processed? 'simple-earley-parser::processed))
  (let ((nt (if nt-name (gethash nt-name (cdr *chinese-g*)) nil)))
    (remove-if-not #'(lambda (p)
                       (and (or (null start)
                                (= start (simple-earley-parser::partial-parse-start p)))
                            (or (null nt)
                                (eq nt (simple-earley-parser::partial-parse-head p)))
                            (or (null processed?)
                                (eq processed? (simple-earley-parser::partial-parse-processed? p)))))
                 (simple-earley-parser::parses-completed (aref partial-parses-vec at)))))

(defun for-tags-of-parse (str func &optional (start-symbol 's))
  (let ((nt (gethash start-symbol (cdr *chinese-g*))))
    (multiple-value-bind (best-parses best-score p-vec)
        (test-parse str nil start-symbol t)
      (declare (ignore best-parses))
      (when best-score
        ;; has any parse at all
        (dolist (p (simple-earley-parser::parses-completed
                    (aref p-vec (- (length p-vec) 1))))
          ;; only those completed, spanning the whole string, having best score
          (when (and (= 0 (simple-earley-parser::partial-parse-start p))
                     (eq nt (simple-earley-parser::partial-parse-head p))
                     (simple-earley-parser::partial-parse-score p)
                     (= best-score
                        (simple-earley-parser::partial-parse-score p))
                     )
            (for-tags-of-partial-parse p func)))))))

(defun tags-of-parse (str &optional (start-symbol 's))
  (let ((ts nil))
    (for-tags-of-parse str
                       #'(lambda (x) (push x ts))
                       start-symbol)
    ts))

(defmacro tag-to (var str &optional (start-symbol 's))
  `(defparameter ,var (car (tags-of-parse ,str ,start-symbol))))
