;;;
;;; Note the that current testing grammar is looser than real usage, i.e. it accepts sentences that are no common
;;; But we hope that this does not add too much unnecessary ambiguity in the parsing
(defun test-penalty-func2 (grammar-word input-words)
  ;; penalty of -100 for both mismatch, insertion and deletion
  (let ((input-word (car input-words)))
    (cond ((null grammar-word) *c-null-grammar-word*)
	  ((null input-word) *c-null-input-word*)
	  ((equalp grammar-word input-word) 0)
	  ((eq grammar-word :xx) *c-start-unknown*) ;; start of unknown part
	  ((eq grammar-word :x) *c-cont-unknown*) ;; continuation of unknown part
	  (t *c-mismatch*))))
  
(defun test-penalty-func (grammar-word input-words)
  (let ((input-word (car input-words)))
    (cond ((null grammar-word) nil)
          ((null input-word) nil)
          ((characterp grammar-word) (cn-char-equal grammar-word input-word))
          ((eq grammar-word :xx) *c-start-unknown*) ;; start of unknown part
          ((eq grammar-word :x) *c-cont-unknown*) ;; continuation of unknown part
          ((eq grammar-word :name) (match-name input-words))
          ((eq grammar-word :human-name) (match-human-name input-words))
          ((eq grammar-word :animate) (match-tagged-word-hash *animate-hash* input-words))
          ((eq grammar-word :animate-suffix) (match-tagged-word-hash *animate-suffix-hash* input-words))
          ((eq grammar-word :inanimate) (match-tagged-word-hash *inanimate-hash* input-words))
          ((eq grammar-word :place) (match-tagged-word-hash *places-hash* input-words))
          ((eq grammar-word :abstract) (match-tagged-word-hash *abstract-hash* input-words))
          ((eq grammar-word :abstract-suffix) (match-tagged-word-hash *abstract-suffix-hash* input-words))
          ((eq grammar-word :noun-suffix) (match-tagged-word-hash *noun-suffix-hash* input-words))
          ((eq grammar-word :dir) (match-tagged-word-hash *dirs-hash* input-words))
          ((eq grammar-word :dir-suffix) (match-tagged-word-hash *dir-suffix-hash* input-words))
          ((eq grammar-word :unit) (match-aa-tagged-word-hash *units-hash* input-words))
          ((eq grammar-word :pronoun) (match-tagged-word-hash *pronouns-hash* input-words))
          ((eq grammar-word :q-pronoun) (match-tagged-word-hash *q-pronouns-hash* input-words))
          ((eq grammar-word :p-pronoun) (match-tagged-word-hash *p-pronouns-hash* input-words))
          ((eq grammar-word :verb) (match-verb-hash *verbs-hash* input-words))
          ((eq grammar-word :help-verb) (match-verb-hash *help-verbs-hash* input-words))
          ((eq grammar-word :dir-verb) (match-verb-hash *dir-verbs-hash* input-words))
          ((eq grammar-word :verb-supp) (match-tagged-word-hash *verb-close-supp-hash* input-words))
          ((eq grammar-word :adj) (match-adj-hash *adjectives-hash* input-words))
          ((eq grammar-word :adv) (match-aa-tagged-word-hash *adverbs-hash* input-words))
          ((eq grammar-word :front-adv) (match-tagged-word-hash *front-adverbs-hash* input-words))
          ((eq grammar-word :conn) (match-word-list *conns-list* input-words))
          ((eq grammar-word :exclaim) (match-word-list *exclaims-list* input-words))
          ((eq grammar-word :double-char) (match-double-char input-words))
          ((eq grammar-word :prep) (match-word-list *preps-list* input-words))
          ((eq grammar-word :dynasty) (match-tagged-word-hash *dynasties-hash* input-words))
          ((eq grammar-word :sound) (match-aa-word-list *sounds-list* input-words))
          ((eq grammar-word :time) (match-tagged-word-hash *times-hash* input-words))
          ((eq grammar-word :surname) (match-tagged-word-hash *surnames-hash* input-words))
          (t nil))))
