(defun tree-diff (a b &optional (marker 'xxx))
  ;; a and b are two cons trees, replace the parts different in a and b with marker
  (cond ((null a) (if (null b) nil marker))
	((null b) marker)
	((consp a) (if (consp b)
		       (cons (tree-diff (car a) (car b) marker)
			     (tree-diff (cdr a) (cdr b) marker))
		       marker))
	((consp b) marker)
	((equalp a b) a)
	(t marker)))

