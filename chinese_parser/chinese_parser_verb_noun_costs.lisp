;; start *verb-noun-costs*

(defun tmp-x (entry) ;; ((verb n) cost)
  (if (consp entry)
      (let ((v (caar entry))
            (n (cadar entry))
            (cost (cadr entry)))
        (cond ((and (eq cost '*c-common*)
                    (not (member n '(abstract noun
                                     physical verb-n)))
                    (not (string= "V-" (symbol-name n) :end2 2)))
               `((,v ,n) *c-close-suffix*))
              ;;((and (eq cost '*c-close-suffix*)
              ;;      (eq n 'verb-n))
              ;; `((,v ,n) *c-common*))
              (t entry)))
      entry))
;; ;; *tmp* is the list of def-cost-comb below
(defparameter *tmp* nil)
(defparameter *tmp1* (remove-if-not #'consp *tmp*))
(defparameter *tmp2*
  (mapcar #'(lambda (x)
              `(,(first x) ,(second x)
                 ,@(mapcar #'tmp-x (cddr x))))
          *tmp1*))

