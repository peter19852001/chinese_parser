;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Since we basically let each verb be its own class, the verb-noun
;; pairs extracted from CTB is somewhat sparse, we now use a quick and
;; dirty way to group verbs with the same last character together,
;; possibly with a little human inspection and correction. By doing
;; this, we hope that the verb-noun pairs of at least some verbs are
;; shared.

(defun hash-to-alist (h)
  (let ((r nil))
    (maphash #'(lambda (k v) (push (cons k v) r)) h)
    r))

;; get the verbs that has verb-noun pairs here.
;; group by the last character
#+(or)
(defparameter *verbs-with-same-last-char*
  (let ((by-last-char (make-hash-table))
        (res nil))
    (labels ((process-fn (p)
               (let ((word (car p))
                     (verb-tag-name (type-of (cdr p))))
                 ;; word is a list of character
                 (when (gethash verb-tag-name *verb-noun-costs*)
                   ;; has verb-noun pair
                   (let ((c (car (last word))))
                     (push (cons (coerce word 'string)
                                 verb-tag-name)
                           (gethash c by-last-char nil)))))))
      (for-word-list-hash *verbs-hash* #'process-fn)
      (for-word-list-hash *help-verbs-hash* #'process-fn)
      (for-word-list-hash *dir-verbs-hash* #'process-fn)
      ;; only retain those with more than one verb using a character,
      ;; also convert the character into string, so that it is easier
      ;; to see in REPL.  Also keep only those where the single
      ;; character is also a verb, put the verb at the front.
      (maphash #'(lambda (k v)
                   (when (> (length v) 1)
                     (let ((w (string k)))
                       (when (find w v :key #'car :test #'string=)
                         (push (sort v #'< :key #'(lambda (x) (length (car x))))
                               res)))))
               by-last-char)
      ;;
      res)))

