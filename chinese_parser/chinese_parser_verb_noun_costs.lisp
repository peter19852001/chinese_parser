(in-package :chinese-parser)

;; for sbcl when loading utf-8 encoded file
(setf sb-impl::*default-external-format* :utf-8)

;; verb-tag-name => ((noun1-tag-name . cost1) (noun2-tag-name . cost2) ...)

;; The list mostly contain (noun-tag-name . cost) pair, but may
;; contain only a symbol, which indicate to look recursively into the
;; list for that symbol. This is for some common pairs shared among
;; many verbs, but allow any specific pairs for a particular noun to
;; appear early, and therefore override the shared ones.
(defparameter *verb-noun-costs* (make-hash-table :test 'eq))

(defun add-cost-comb (cost-table tag-key tag-val cost)
  ;; NOTE: only add to the current list, not to the recursive list, so
  ;; that any specific (noun . cost) can be added to override the
  ;; default.
  
  ;; new cost will override old one for the same pair of tag-key and tag-val
  (let ((p (find-if #'(lambda (p)
                        ;; only check the (noun-tag . cost) pairs
                        (and (consp p) (eq tag-val (car p))))
                    (gethash tag-key cost-table nil))))
    (if p
        (setf (cdr p) cost)
        (push (cons tag-val cost) (gethash tag-key cost-table nil)))))

(defun which-cost-comb (cost-table tag-key tag-val)
  ;; NOTE: may need to find recursively
  ;; NOTE: assume there are no cycles!
  (dolist (p (gethash tag-key cost-table nil))
    (cond ((consp p)
           ;; p is (noun-tag . cost)
           (when (subtypep tag-val (car p))
             (return-from which-cost-comb p)))
          ((symbolp p)
           ;; recursive
           (let ((r (which-cost-comb cost-table p tag-val)))
             (when r (return-from which-cost-comb r)))))))

(defun get-cost-comb (cost-table tag-key tag-val &optional default-cost)
  ;; the first value is the cost, use the default-cost if not found.
  ;; the second value is whether the entry is found.
  (let ((p (which-cost-comb cost-table tag-key tag-val)))
    (if p
        (values (cdr p) t)
        (values default-cost nil))))

(defmacro def-cost-comb (cost-table &body clauses)
  ;; note that this can be used multiple times on the same cost-table
  ;; and the entries will accumulate, and newer entries will override old ones for the same pair.
  (let ((n-table (gensym)))
    `(let ((,n-table ,cost-table))
       ,@(mapcar #'(lambda (c)
                     ;; to ignore the comments kept as strings
                     (if (consp c)
                         `(add-cost-comb ,n-table ',(caar c) ',(cadar c) ,(cadr c))
                         nil))
                 clauses))))

