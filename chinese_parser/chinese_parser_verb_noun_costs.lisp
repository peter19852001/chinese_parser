;; update the verb-noun pairs for sharing
(defun union-verb-noun-pairs (verbs)
  ;; verbs is a list of (word . verb-name), e.g. ("出入" . V-CHU1-RU4)
  ;; which shoud have similar verb-noun constraints.
  ;; union all their verb-noun constraints which have better scores than *c-common*
  ;; each verb-noun constraints becomes only a (noun-type-symbol . cost) pair.
  ;; If there are duplicate noun-type-symbol, take the better cost.
  (let ((nouns nil)
        ;; costs: noun-type-symbol => cost
        (costs (make-hash-table :test 'eq)))
    (dolist (vp verbs)
      ;; vp is (word . verb-type-symbol)
      (dolist (np (gethash (cdr vp) *verb-noun-costs*))
        ;; only handle case where np is (noun-type-symbol . cost) and
        ;; cost is better than *c-common*
        (when (and (consp np)
                   (cdr np)
                   (> (cdr np) *c-common*))
          (let* ((ns (car np))
                 (new-cost (if (= (cdr np) 0)
                               ;; promote too low cost, because this is shared, to avoid too much influence
                               *c-close-suffix*
                               (cdr np)))
                 (cs (gethash ns costs)))
            (if cs
                ;; update score if needed, on already seen noun
                (setf (gethash ns costs) (score-max cs new-cost))
                (progn
                  ;; new noun
                  (setf (gethash ns costs) new-cost)
                  (push ns nouns)))))))
    ;; since the original verb-noun cost pairs are from most specific
    ;; to least specific, so reverse nouns and associate the best cost.
    (mapcar #'(lambda (n) (cons n (gethash n costs)))
            (nreverse nouns))))

(defun add-indirection-to-tail (cost-table tag-key tag-indirect)
  ;; specialized
  ;; add to tail, but before the usual (abstract . *c-common*) pair.
  ;; modify in place.
  (let ((ps (gethash tag-key cost-table)))
    (cond ((null ps)
           ;; empty, just add it
           (setf (gethash tag-key cost-table) (list tag-indirect)))
          ((member tag-indirect ps)
           ;; already there, do nothing
           nil)
          (t
           (let ((tail-pos
                  (position-if #'(lambda (p)
                                   (and (consp p)
                                        (eq (car p) 'abstract)
                                        (eql (cdr p) *c-common*)))
                               ps)))
             (setf (gethash tag-key cost-table)
                   (append (subseq ps 0 tail-pos)
                           (cons tag-indirect
                                 (if tail-pos
                                     (subseq ps tail-pos)
                                     nil)))))))))

(defun share-verb-noun-pairs (verbs-list)
  ;; verbs-list is a list of list of (word . verb-type-symbol).

  ;; for
  ;; each group of verbs, find their union (noun . cost) pairs, make a
  ;; symbol name for it, add to *verb-noun-costs*, and modify each of
  ;; the verb-noun list for the verb to also refer to the union list.
  (dolist (vs verbs-list)
    ;; for each group of verbs, their pairs are to be shared among themselves
    (let* ((first-verb-sym (cdar vs))
           ;; make a new name from the first verb
           (union-name (intern (format nil "S-~A"
                                       first-verb-sym)
                               (symbol-package first-verb-sym))))
      ;; add new entry to *verb-noun-costs* for the unioned pairs
      (setf (gethash union-name *verb-noun-costs*)
            ;; find their union (noun . cost) pairs
            (union-verb-noun-pairs vs))
      ;; modify each verb's pair list
      (dolist (vp vs)
        ;; vp is (word . verb-type-symbol)
        (add-indirection-to-tail *verb-noun-costs* (cdr vp) union-name)))))

(share-verb-noun-pairs *verbs-with-same-last-char-filtered*)
